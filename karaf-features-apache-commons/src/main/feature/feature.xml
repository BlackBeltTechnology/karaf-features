<features name="apache-commons-${project.version}"
    xmlns="http://karaf.apache.org/xmlns/features/v1.5.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://karaf.apache.org/xmlns/features/v1.5.0 http://karaf.apache.org/xmlns/features/v1.5.0">

    <feature name="apache-commons-beanutils" version="1.9.4" description="Apache Commons Bean Utils" install="false">
        <feature>apache-commons-collections</feature>
        <bundle>mvn:commons-beanutils/commons-beanutils/1.9.4</bundle>
    </feature>

    <feature name="apache-commons-collections" version="3.2.2" description="Apache Commons Collections" install="false">
        <bundle>mvn:commons-collections/commons-collections/3.2.2</bundle>
    </feature>

    <feature name="apache-commons-collections4" version="4.4" description="Apache Commons Collections4" install="false">
        <bundle>mvn:org.apache.commons/commons-collections4/4.4</bundle>
    </feature>
    
    <feature name="apache-commons-io" version="2.11.0" description="Apache Commons IO" install="false">
        <bundle>mvn:commons-io/commons-io/2.11.0</bundle>
    </feature>
    
    <feature name="apache-commons-pool" version="1.6" description="Apache Commons Pool" install="false">
        <bundle>mvn:commons-pool/commons-pool/1.6</bundle>
    </feature>
    
    <feature name="apache-commons-pool2" version="2.11.1" description="Apache Commons Pool2" install="false">
        <bundle>mvn:org.apache.commons/commons-pool2/2.11.1</bundle>
    </feature>
    
    <feature name="apache-commons-dbcp2" version="2.9.0" description="Apache Commons DBCP2" install="false">
        <feature>transaction-api</feature>
        <feature>apache-commons-pool2</feature>
        <bundle>mvn:org.apache.commons/commons-dbcp2/2.9.0</bundle>
    </feature>
    
    <feature name="apache-commons-codec" version="1.15" description="Apache Commons Codec" install="false">
        <feature>apache-commons-logging</feature>
        <bundle>mvn:commons-codec/commons-codec/1.15</bundle>
    </feature>
    
    <feature name="apache-commons-fileupload" version="1.4" description="Apache Commons Fileupload" install="false">
        <feature>http</feature>
        <feature>apache-commons-logging</feature>
        <feature>apache-commons-io</feature>
        <bundle>mvn:commons-fileupload/commons-fileupload/1.4</bundle>
    </feature>
    
    <feature name="apache-commons-lang" version="2.6" description="Apache Commons Lang" install="false">
        <feature>apache-commons-logging</feature>
        <bundle>mvn:commons-lang/commons-lang/2.6</bundle>
    </feature>
    
    <feature name="apache-commons-logging" version="1.2" description="Apache Commons Logging" install="false">
        <bundle>mvn:commons-logging/commons-logging/1.2</bundle>
    </feature>
    
    <feature name="apache-commons-configuration" version="1.10" description="Apache Commons Configuration" install="false">
        <feature>apache-commons-lang</feature>
        <bundle>mvn:commons-configuration/commons-configuration/1.10</bundle>
    </feature>

    <feature name="apache-commons-configuration2" version="2.8.0" description="Apache Commons Configuration2" install="false">
        <feature>apache-commons-lang3</feature>
        <feature>apache-commons-logging</feature>
        <feature>apache-commons-text</feature>
        <bundle>mvn:org.apache.commons/commons-configuration2/2.8.0</bundle>
    </feature>

    <feature name="apache-commons-math3" version="3.6.1" description="Apache Commons Math3" install="false">
        <bundle>mvn:org.apache.commons/commons-math3/3.6.1</bundle>
    </feature>
    
    <feature name="apache-commons-math" version="2.2" description="Apache Commons Math" install="false">
        <bundle>mvn:org.apache.commons/commons-math/2.2</bundle>
    </feature>
    
    <feature name="apache-commons-lang3" version="3.12.0" description="Apache Commons Lang3" install="false">
        <bundle>mvn:org.apache.commons/commons-lang3/3.12.0</bundle>
    </feature>
    
    <feature name="apache-commons-csv" version="1.9.0" description="Apache Commons CSV" install="false">
        <bundle>mvn:org.apache.commons/commons-csv/1.9.0</bundle>
    </feature>
    
    <feature name="apache-commons-compress" version="1.21" description="Apache Commons Compress" install="false">
        <bundle>mvn:org.apache.commons/commons-compress/1.21</bundle>
    </feature>

    <feature name="apache-commons-bcel" version="5.2" description="Apache Common BCel" install="false">
        <bundle>mvn:org.apache.servicemix.bundles/org.apache.servicemix.bundles.bcel/5.2_4</bundle>
    </feature>

    <feature name="apache-xmlbeans2" version="2.6.0" description="Apache XMLBeans" install="false">
        <bundle>mvn:org.apache.servicemix.bundles/org.apache.servicemix.bundles.xmlbeans/2.6.0_2</bundle>
    </feature>

    <feature name="apache-xmlbeans3" version="3.1.0" description="Apache XMLBeans 3" install="false">
        <bundle>mvn:org.apache.servicemix.bundles/org.apache.servicemix.bundles.xmlbeans/3.1.0_2</bundle>
    </feature>

    <feature name="apache-jexl3" version="3.2.1" description="Apache JEXL3" install="false">
        <bundle>mvn:org.apache.commons/commons-jexl3/3.2.1</bundle>
    </feature>

    <feature name="apache-xmlbeans5" version="5.1.1" description="Apache XMLBeans 5" install="false">
        <bundle>mvn:org.apache.servicemix.bundles/org.apache.servicemix.bundles.xmlbeans/5.1.1_1</bundle>
    </feature>


    <feature name="apache-commons-text" version="" description="Apache Commons text" install="false">
        <feature>apache-commons-lang3</feature>
        <bundle>mvn:org.apache.commons/commons-text/1.10.0</bundle>
    </feature>

</features>
